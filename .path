#/bin/bash

[[ -d /usr/local/opt/go/libexec/bin ]] && export PATH=$PATH:/usr/local/opt/go/libexec/bin
[[ -d "$HOME/go/bin" ]] && export PATH=$HOME/go/bin:$PATH
[[ -z "${JAVA_HOME}" ]] && export JAVA_HOME=$(readlink -f /usr/bin/javac | sed "s:/bin/javac::")
[[ -d "$HOME/opt/pig/bin" ]] && export PATH=$HOME/opt/pig/bin:$PATH
